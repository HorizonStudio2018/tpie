# The directories storing the components of the system

LIBPATH = ./lib

APPSPATH = ./apps

INCPATH = ./include

APPS: LIB INC
	cd $(APPSPATH); $(MAKE)

LIB:
	cd $(LIBPATH); $(MAKE)

INC:
	cd $(INCPATH); $(MAKE)

    