LIBRARY = libtpie.a

LIBSRCPATH = ./src/

RANLIB = ranlib

export


all: $(LIBRARY)

$(LIBRARY): $(LIBSRCPATH)$(LIBRARY)
	cp $< $@
	$(RANLIB) $@

.PHONY : clean all

clean:
	-rm $(LIBRARY)
	-rm $(UP_TO_DATE)
	$(MAKE) -C $(LIBSRCPATH) $@




