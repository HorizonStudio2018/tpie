LIBRARY = libtpie.a

LIBSRCPATH = ./src/

RANLIB = @RANLIB@

export


all: $(LIBRARY)

$(LIBRARY): $(LIBSRCPATH)$(LIBRARY)
	cp $< $@
	$(RANLIB) $@

.PHONY : clean all

clean:
	-rm $(LIBRARY)
	$(MAKE) -C $(LIBSRCPATH) $@




